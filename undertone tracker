<script type='text/javascript'>
  window.onload = function(){
    var p=location.protocol=='https:'?'https:':'http:';
    var r=Math.floor(Math.random()*999999);
    var ut_img = document.createElement("img");
    ut_img.src = p+"//ads.undertone.com/t?trackerid=7579&cb="+r;
    ut_img.alt = "";
    ut_img.style = "display:none";
    ut_img.border = 0;
    ut_img.height = 1;
    ut_img.width = 1;
    document.body.appendChild(ut_img);
  }
</script>
